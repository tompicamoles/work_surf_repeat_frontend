{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.removeUnusedImports": "explicit"
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#fbf4f4",
    "activityBar.background": "#fbf4f4",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#5bc35b",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#fbf4f4",
    "statusBar.background": "#efcdce",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#e3a6a8",
    "statusBarItem.remoteBackground": "#efcdce",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#efcdce",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#efcdce99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#efcdce"
}
